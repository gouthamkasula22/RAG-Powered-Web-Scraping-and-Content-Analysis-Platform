{
    "include": [
        "src",
        "backend",
        "frontend",
        "tests"
    ],
    "exclude": [
        "**/node_modules",
        "**/__pycache__",
        "htmlcov"
    ],
    "ignore": [
        "build"
    ],
    "defineConstant": {
        "DEBUG": true
    },
    "stubPath": "typings",
    "venvPath": ".",
    "venv": ".venv",
    "reportMissingImports": "warning",
    "reportMissingTypeStubs": false,
    "pythonVersion": "3.11",
    "pythonPlatform": "All",
    "executionEnvironments": [
        {
            "root": "src"
        },
        {
            "root": "backend"
        },
        {
            "root": "frontend"
        },
        {
            "root": "tests"
        }
    ]
}
